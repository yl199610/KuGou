        }
        focusNum++;
        return;
    }

    keywords = Kg.$('ipt').value.trim();
    searchPage = 1;
    searchWord();
}

function searchWord(){
	var word = keywords.replace(/\(.*\)/g, '');

	Kg.getJSON(kg_domain + "singer/name/w=" +  encodeURIComponent(word) +"&s=1000&all=1&n=" + Math.random(),{},function(o){
		if(o.data.length==1){
			location.href= kg_domain +  'singer/home/' + o.data[0].singerid + '.html';
			return;
		};

		var odd='';
		var counter=1;
		var pos=1;
		var contents='';
		var itemCount = o.data.length;
		var headPageCount = Math.ceil(itemCount / 4);
		for(var j = 1; j <= headPageCount; j++){
			contents += "<ul class='list1'>";
			var hlen = itemCount - j * 4 > 0 ? 4 * j : itemCount;
			for(var i=(j-1)*4;i<hlen;i++){
				var updown='normal';
				var title = "排名没变化";
				if(+o.data[i].SortOffset<0){
					updown='down';
					 title = "下降了" + Math.abs(o.data[i].SortOffset) + "位";
				} 
				if(+o.data[i].SortOffset>0){
					updown='up';
					title = "上升了" + Math.abs(o.data[i].SortOffset) + "位";
				} 

				if(counter % 2 != 0){
					odd='class="odd"';
				}else{
					odd='';
				}

				if(o.data[i].cindex != null){
					contents+='<li title="' + o.data[i].singername + '" '+odd+'><span class="ran">'+(pos++)+'</span><span title="' + title + '"></span><a onclick="sdnClick(12071)" href="' + kg_domain + 'singer/home/' + o.data[i].singerid + '.html" class="text">'+o.data[i].singername+'</a></li>';
				}
				counter++;
			}
			contents += "</ul>";
		}

		Kg.$('list_head').style.display = "none";
		head1 = '搜索:' + keywords + '&nbsp;&nbsp;共' + o.data.length + '条记录<br><hr>';
		Kg.$('list1').innerHTML =head1+contents;
		Kg.$('nvg').style.display = "none";
		Kg.$("poplist").innerHTML = "";
		Kg.$("poplist").style.display = "none"; 
		issearch = 1;
	});
}

function searchFromUrl(){
    var search = location.search;
    if(search){
        search = search.replace("?","");
        var res = new Kg.Param().parse(search);
        if(res.keywords != null){
            keywords = decodeURIComponent(res.keywords);
			Kg.$("ipt").value = keywords;
			searchPage =1;
            searchWord();
        } 
    }
}

function init(){
	var el = document.getElementById("ipt");
	el.onfocus = function(){
		if(this.value == this.defaultValue){
			this.value = ""	;
		}	
	}
	el.onblur = function(){
		if(this.value == ""){
			this.value = this.defaultValue;
		}	
	}
    
    Kg.addEvent(el, "keyup", function(e){
		 var myEvent = e || windows.event;  
		 var keyCode = myEvent.keyCode || myEvent.which;
		 if(keyCode != 38 && keyCode != 40 && keyCode != 13){  
			highLightIndex = -1; 

			var el = Kg.$("ipt");
			var word = el.value;
			if(word != el.defaultValue && word.length > 0){
				if(word.trim() != lastSearch){
					Kg.postJSON(kg_domain + "singer/name/w="+ encodeURI(word.trim()) +"&s=10&n=" + Math.random(), {},function(json){
						if(json.status == 1 && json.data.length > 0){
							var popEl = Kg.$("poplist");
							if(json.data.length != 0) {
							   popEl.innerHTML = "";
							   popEl.style.display = "block";   
							   for(var i = 0, l = Math.min(json.data.length, 10); i < l ; i++) {
								  var word_div = document.createElement("div"); 
								   word_div.id = json.data[i].singerid;
								   word_div.innerHTML = word_div.title = json.data[i].singername.replace(/"/g,'&quot;').replace(/'/g,'&#039;');
								   word_div.onmouseover = fnOver;
								   word_div.onmouseout = fnOut;
								   word_div.onclick = getAutoText;
								   popEl.appendChild(word_div);
							   } 
							} else {
								popEl.innerHTML = "";
								popEl.style.display = "none";
							}
						} else {
							var popEl = Kg.$("poplist");
							popEl.style.display = "none";
						}
					})
				}
				lastSearch = word.trim();
			} else {
				var popEl = Kg.$("poplist");
				popEl.style.display = "none";
			}
			lastSearch = word;

		 }else if(keyCode == 38 || keyCode == 40){  
			processKeyUpAndDown(keyCode);  
		 } else if(keyCode == 13){
			getAutoText()
		 }
	});
    
	searchFromUrl();
	new Lazyload();
}

document.onclick=function(event){
    try{
        Kg.$("poplist").style.display = "none"; 
		highLightIndex = -1;
    } catch(ex) {}
}

init();

</script>
